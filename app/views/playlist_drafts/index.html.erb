<p id="notice"><%= notice %></p>

<h1>Playlist Drafts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playlist_drafts.each do |playlist_draft| %>
      <tr>
        <td><%= link_to 'Show', playlist_draft %></td>
        <td><%= link_to 'Edit', edit_playlist_draft_path(playlist_draft) %></td>
        <td><%= link_to 'Destroy', playlist_draft, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Playlist Draft', new_playlist_draft_path %>
